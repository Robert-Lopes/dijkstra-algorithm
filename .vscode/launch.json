{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug FastAPI (uvicorn)",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",        
                "--reload",
                "--host", "127.0.0.1",
                "--port", "5000"
            ],
            "jinja": true,
            "console": "integratedTerminal"
        }
    ]
}
